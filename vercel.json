{
  "version": 2,
  "builds": [
    {
      "src": "prototype/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "my_crewai_project/api_wrapper.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/crew-python",
      "dest": "/my_crewai_project/api_wrapper.py"
    },
    {
      "src": "/(.*)",
      "dest": "/prototype/$1"
    }
  ],
  "env": {
    "GROQ_API_KEY": "@groq_api_key",
    "GROQ_MODEL_NAME": "@groq_model_name"
  },
  "functions": {
    "my_crewai_project/api_wrapper.py": {
      "runtime": "python3.9"
    }
  }
} 